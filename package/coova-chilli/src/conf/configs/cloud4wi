config general 'hotspot1'
  option profile 'cloud4wi'
	option enabled '0'
	option net '192.168.2.254/24'
	option addvert_address 'https://www.teltonika-networks.com'
	option mode 'extrad'
	option auth_proto 'pap'
	option radiusserver1 '54.247.117.188'
	option radiusserver2 '79.125.111.180'
	option externalpage '1'
	option externadress 'https://splashportal.cloud4wi.com?vendor=teltonika'

config ftp 'ftp'
	option enabled '0'
	option host 'your.ftp.server'
	option user 'username'
	option psw 'password'
	option port '21'
	option extra_name_info 'none'

config interval
	option descr 'Uploads every workday at 8:15'
	option fixed '1'
	option fixed_hour '8'
	option fixed_minute '15'
	option weekdays 'mon,tue,wed,thu,fri'

config link 'link'

config session 'unlimited1'
	option name 'unlimited'
	option id 'hotspot1'

config uamallowed
	option instance 'hotspot1'
	option enabled '1'
	option domain 'cloud4wi.com'
	option subdomains '1'

